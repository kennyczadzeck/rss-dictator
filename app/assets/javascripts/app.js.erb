var app = angular.module("rssDictator", ["ngResource", "ngRoute"]);

app.config(['$routeProvider',
  function($routeProvider) {
    $routeProvider.
      when('/', {
        templateUrl: '<%= asset_path("templates/main.html") %>',
        controller: 'MainController'
      }).
      otherwise({
        redirectTo: '/'
      });
  }]);