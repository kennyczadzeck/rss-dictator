<div style="text-align:center">
  <h1>Add RSS/Atom Feed</h1>
  <br>
  <form name="newFeedForm" ng-submit="newFeedForm.$valid && createFeed()" novalidate>
    <div>
      <input type="text" ng-model="feedUrl" required class="form-control" placeholder="Enter Url here..."><br>
      <input type="submit" class="btn btn-primary">
    </div>
  </form>
</div>
<div>
  <h1 style="text-align:center">Remove RSS/Atom Feed</h1>
  <table class="table table-border table-hover">
    <tr ng-repeat="feed in feeds">
      <td>
        {{feed.title}}
      </td>
      <td><button ng-click="deleteFeed(feed.id)" class="btn btn-danger">Delete</button></td>
    </tr>
  </table>
</div>