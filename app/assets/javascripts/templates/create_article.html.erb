<div style="text-align:center">
  <h1>Add Article</h1>
  <br>
  <form name="newArticleForm" ng-submit="newArticleForm.$valid && createArticle()" novalidate>
    <div>
      <input type="text" ng-model="articleUrl" required class="form-control" placeholder="Enter Url here..."><br>
      <input type="submit" class="btn btn-primary">
    </div>
  </form>
</div>